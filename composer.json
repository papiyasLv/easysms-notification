{
    "name": "papiyas/easysms-notification",
    "description": "EasySms Notification Channel for Laravel.",
    "keywords": ["laravel", "notifications", "easysms"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "papiyas",
            "email": "papiyas@126.com"
        }
    ],
    "require": {
        "overtrue/easy-sms": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Papiyas\\Notifications\\EasySms\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Papiyas\\Notifications\\EasySms\\EasySmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
